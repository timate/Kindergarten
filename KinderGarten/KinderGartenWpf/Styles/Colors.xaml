<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!-- #region White Color One-->
    <Color x:Key="Background">#fbfbfb</Color>
    <SolidColorBrush x:Key="BackgroundBrush" Color="{StaticResource Background}"/>
    <!--#endregion-->

    <!--#region Gray Color Two-->
    <Color x:Key="BackgroundDark" R="0" G="0" B="0" A="10"/>
    <SolidColorBrush x:Key="BackgroundDarkBrush" Color="{StaticResource BackgroundDark}"/>
    <!--#endregion-->
    
    <!--#region LightDark Color Three, ColorFour-->
    <Color x:Key="Foreground" R="0" G="0" B="0" A="200"/>
    <SolidColorBrush x:Key="ForegroundBrush" Color="{StaticResource Foreground}"/>
    <!--#endregion-->
    
    <!--#region Accent Color-->
    <Color x:Key="Accent">#3895d3</Color>
    <SolidColorBrush x:Key="AccentBrush" Color="{StaticResource Accent}"/>
    <!--#endregion-->

    <!--#region Primary Color-->
    <Color x:Key="Primary">#43dbe4</Color>
    <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource Primary}"/>
    <!--#endregion-->

</ResourceDictionary>